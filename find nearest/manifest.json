{
    "manifest_version": 3,
    "name": "Find Nearby bar",
    "version": "1.0",
    "description": "Find the nearest bar to your location.",
    "options_page": "options.html",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "geolocation", "storage"
    ]
}
